//Стоимость яблока составляет 2$. Покупатель приобретает 6 яблок. Напишите программу которая вычислит и выведет на экран сумму которую должен уплатить покупатель за покупку.

package sample;
  
 
	public class Main{ 


    	  public static void main(String[] args) {
    	    int cena = 2;
			int ks =6;
    	   Object kup = ks +cena;
    			   
    	System.out.println( kup +  " dolar" );
    	 
    }
  }
   
